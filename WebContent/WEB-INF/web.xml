<?xml version="1.0" encoding="UTF-8"?>
<web-app
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">
 	<servlet>
    	<servlet-name>FirstServlet</servlet-name>
    	<servlet-class>com.bases.myServlets.FirstServlet</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>FirstServlet</servlet-name>
    	<url-pattern>/firstServletUse</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>ServletJDBC</servlet-name>
    	<servlet-class>com.bases.myServlets.servletJDBC</servlet-class>
	</servlet>
	<servlet-mapping>
    	<servlet-name>ServletJDBC</servlet-name>
    	<url-pattern>/firstjdbc</url-pattern>
	</servlet-mapping>
	
</web-app>